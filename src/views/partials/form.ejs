<div class="container-form">
    <form action="/datos" method="post" class="box-form" id="dataForm">
        <label for="">Nombre</label>

        <input type="text" required name="nombre" placeholder="Nombre del producto">

        <label for="">Marca</label>

        <input type="text" required  name="marca" placeholder="Nombre de la marca ej:sony,adidas">
    
        <label for="">Precio</label>

        <input type="number" required  name="precio" id="" placeholder="Colocar el precio sin comas,ni puntos">
    
        <label for="">url de la imagen</label>

        <input type="text" required  name="image" placeholder="URL de la imagen">
    
        <label for="">stock del producto</label>

        <input type="number" required  name="stock" placeholder="Cantidad de unidades disponibles">

        <button type="submit">enviar</button>
    </form>
</div>